/* component/settings/settings.wxss */
.settings{
  width:750rpx;
  height: 100rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0rpx 30rpx;
  box-sizing: border-box;
}
.settings .title{
  font-size: 36rpx;
}
.settings .date{
  font-size: 27rpx;
  color: #999;
}
.settings .arrow{
  width: 40rpx;
  height: 40rpx;
}